{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "Generator/**",
            "VkGenerator/**",
            "OpenAL/OpenALGenerator/**",
            "OpenAL/OpenTK.OpenAL.Extensions/**"
          ],
          "src": "opentk/src"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "learn/**.md",
          "learn/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "favicon.ico",
          "**/*.png",
          "**/*.jpg",
          "**/*.mp4"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../Website_GhPages",
    "globalMetadata": {
      "_appTitle": "OpenTK",
      "_appName": "",
      "_disableContribution": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./overrides"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
